@model MovieListAndCheckout

@{
    ViewData["Title"] = "Movie Checkout";
}

<h1>Checkout Confirmation</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Title
            </th>
            <th>
               Due Date
            </th>
        </tr>
    </thead>
    <tbody>
            <tr>
                <td>
                    @Model.MovieTitle
                </td>
                <td>
                    @Model.MovieDueDate
                </td>
            </tr>
    </tbody>
</table>